{
  "ReverseProxy": {
    "Routes": {
      "identity": {
        "ClusterId": "identityCluster",
        "Match": {
          "Path": "/identity/{**catch-all}"
        }
      },

      "test-secure": {
        "ClusterId": "testCluster",
        "Match": {
          "Path": "/test/secure"
        },
        "AuthorizationPolicy": "AdminOnly"
      }
    },

    "Clusters": {
      "identityCluster": {
        "Destinations": {
          "d1": {
            "Address": "https://localhost:7069/"
          }
        }
      },

      "testCluster": {
        "Destinations": {
          "d1": {
            "Address": "https://localhost:6001/"
          }
        }
      }
    }
  }
}
